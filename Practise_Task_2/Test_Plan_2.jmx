        <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="Calculate dynamic posts numbers" enabled="true">
          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="script">import java.lang.Math;


def number_of_posts = vars.get(&quot;GENERAL_POST_NUMBER&quot;).toInteger()
def number_of_users = vars.get(&quot;USER_NUMBER&quot;).toInteger()

if (number_of_posts.mod(number_of_users) == 0 ){
	vars.put(&quot;POSTS_NUMBER&quot;, (Math.floor(number_of_posts/number_of_users).toInteger()).toString())
	vars.put(&quot;ADDITIONAL_POSTS_NUMBER&quot;, &quot;0&quot;)
}else
{
	vars.put(&quot;POSTS_NUMBER&quot;, (Math.floor(number_of_posts/number_of_users).toInteger()).toString())
	vars.put(&quot;ADDITIONAL_POSTS_NUMBER&quot;, number_of_posts.mod(number_of_users).toString())
}
</stringProp>
          <stringProp name="scriptLanguage">groovy</stringProp>
